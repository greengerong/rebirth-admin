{
  "name": "rebirth-admin",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "mock": "node ./build/mock-server.js",
    "start": "npm-run-all --parallel mock serve",
    "build": "ng build --prod --aot --base-href=/rebirth-admin/",
    "serve": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "lint-fix": "tslint -c tslint.json \"src/**/*.ts\"  --fix",
    "test": "ng lint && ng test --watch=false  --browsers ChromeHeadless --code-coverage",
    "ut": "ng test --watch=true  --browsers=Chrome",
    "e2e": "ng e2e",
    "i18n:extract": "ng-xi18n",
    "build-electron": "ng build --base-href ./ && cp src/electron/* dist",
    "electron-run": "npm run build-electron && electron dist",
    "electron": "npm-run-all --parallel mock electron-run",
    "electron-pack": "npm run build-electron && electron-packager dist --out dist/electron",
    "docs:serve": "compodoc ./src/app/ -t -p ./src/tsconfig.app.json -b / -d ./compodocs/ --hideGenerator -s",
    "docs:gen": "compodoc ./src/app/ -t -p ./src/tsconfig.app.json -b /rebirth-admin/compodocs/ -d ./dist/compodocs/ --hideGenerator",
    "github:deploy": "npm run build && npm run docs:gen && ngh --message \"rebirth admin deploy\" && rm -rf ./dist",
    "cimmit-message": "node ./build/commit-msg.js",
    "precommit": "lint-staged",
    "commitmsg": "node ./build/commit-msg.js",
    "prepush": "npm test",
    "prettier-watch": "onchange '**/*.ts' -- prettier --write --single-quote --trailing-comma=all {{changed}}",
    "prettier": "prettier --parser typescript --single-quote --trailing-comma=all --write \"./**/*.ts\""
  },
  "lint-staged": {
    "*.{js,css,ts}": [
      "prettier --write --single-quote --trailing-commaall",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@angular/service-worker": "^6.0.0",
    "bootstrap-sass": "^3.3.7",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "normalize.css": "^7.0.0",
    "rebirth-event-source": "^6.0.1",
    "rebirth-http": "^6.0.2",
    "rebirth-ng": "^6.0.3",
    "rebirth-permission": "^6.0.1",
    "rebirth-storage": "^6.0.1",
    "rxjs": "^6.1.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.0",
    "@angular-devkit/build-angular": "^0.6.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "typescript": ">=2.7.0 <2.8.0",
    "@compodoc/compodoc": "^1.0.4",
    "@types/jasmine": "^2.5.54",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.0.25",
    "@types/protractor": "^4.0.0",
    "@types/webpack": "^3.0.10",
    "angular-cli-ghpages": "^0.5.0",
    "codelyzer": "^3.1.2",
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0",
    "gulp-stubby-server": "^0.2.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-istanbul-threshold": "^1.2.2",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.1",
    "onchange": "^3.2.1",
    "prettier": "1.8.2",
    "protractor": "^5.1.2",
    "stubby": "^4.0.0",
    "ts-node": "^3.3.0",
    "tslint": "~5.7.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0"
  }
}
