{
  "name": "rebirth-admin",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:extract": "ng-xi18n",
    "docs:serve": "compodoc ./src/app/exports/ -p ./src/tsconfig.json -b / -d ./compodocs/ --hideGenerator -s",
    "docs:gen": "compodoc ./src/app/exports/ -p ./src/tsconfig.app.json -b /rebirth-admin/compodocs/ -d ./dist/compodocs/ --hideGenerator",
    "demo:gen": "ng build --prod --aot --base-href=/rebirth-admin/",
    "github-pages:push": "ngh --message \"rebirth ui demo deploy\"",
    "docs:deploy": "npm run demo:gen && npm run docs:gen && npm run github-pages:push && rm -rf ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/common": ">=4.0.0-beta <5.0.0",
    "@angular/compiler": ">=4.0.0-beta <5.0.0",
    "@angular/core": ">=4.0.0-beta <5.0.0",
    "@angular/forms": ">=4.0.0-beta <5.0.0",
    "@angular/http": ">=4.0.0-beta <5.0.0",
    "@angular/platform-browser": ">=4.0.0-beta <5.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0-beta <5.0.0",
    "@angular/router": ">=4.0.0-beta <5.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "ng4-rebirth-ui": "0.0.3",
    "rebirth-chart": "0.0.2",
    "rebirth-event-source": "0.0.1",
    "rebirth-http": "0.0.9",
    "rebirth-permission": "0.0.4",
    "rebirth-storage": "0.0.7",
    "rxjs": "^5.1.0",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.2",
    "@angular/compiler-cli": ">=4.0.0-beta <5.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/webpack": "^2.2.11",
    "angular-cli-ghpages": "^0.5.0",
    "codelyzer": "^2.0.1",
    "compodoc": "0.0.41",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.1",
    "typescript": "~2.1.5"
  }
}
