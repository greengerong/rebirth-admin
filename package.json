{
  "name": "rebirth-admin",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "mock": "node ./build/mock-server.js",
    "pwa": "node ./build/sw-precache.js",
    "start": "npm-run-all --parallel mock serve",
    "build": "ng build --prod --aot --base-href=/rebirth-admin/ && npm run pwa",
    "serve": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "lint-fix": "tslint -c tslint.json \"src/**/*.ts\"  --fix",
    "test": "ng lint && ng test --watch=false --single-run=true --code-coverage",
    "e2e": "ng e2e",
    "i18n:extract": "ng-xi18n",
    "docs:serve": "compodoc ./src/app/ -p ./src/tsconfig.app.json -b / -d ./compodocs/ --hideGenerator -s",
    "docs:gen": "compodoc ./src/app/ -p ./src/tsconfig.app.json -b /rebirth-admin/compodocs/ -d ./dist/compodocs/ --hideGenerator",
    "github:deploy": "npm run build && npm run docs:gen && ngh --message \"rebirth admin deploy\" && rm -rf ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "bootstrap-sass": "^3.3.7",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "normalize.css": "^7.0.0",
    "rebirth-chart": "0.0.4",
    "rebirth-event-source": "0.0.1",
    "rebirth-http": "0.0.13",
    "rebirth-ng": "0.0.16",
    "rebirth-permission": "0.0.8",
    "rebirth-storage": "0.0.10",
    "rxjs": "^5.4.1",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.3",
    "@angular/compiler-cli": "^4.2.4",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.3",
    "@types/protractor": "^4.0.0",
    "@types/webpack": "^2.2.11",
    "angular-cli-ghpages": "^0.5.0",
    "codelyzer": "^3.1.1",
    "gulp-stubby-server": "^0.2.0",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.0.2",
    "protractor": "^5.1.2",
    "stubby": "^4.0.0",
    "sw-precache": "^5.2.0",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "2.3.4"
  }
}
